{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": "off",
    "import/no-extraneous-dependencies": "off",
    "no-shadow": "off",
    "dot-notation": "off",
    "no-param-reassign": "off",
    "prettier/prettier": ["error", { "tabWidth": 4 }]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
